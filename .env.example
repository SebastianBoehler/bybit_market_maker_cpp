# Bybit Market Maker example configuration
BYBIT_API_KEY=
BYBIT_API_SECRET=

# REST/WS endpoints
BYBIT_BASE_URL=https://api.bybit.com
BYBIT_CATEGORY=linear
BYBIT_WS_PUBLIC_URL=wss://stream.bybit.com/v5/public/linear

# Strategy controls
BYBIT_RUN_LIVE=0           # set to 1 to place/cancel orders; 0 = dry-run
BYBIT_SYMBOL=BTCUSDT
BYBIT_BUDGET_USD=10.0      # USD budget used to size both sides
BYBIT_MIN_SPREAD_BPS=1.0   # floor spread in bps
BYBIT_SPREAD_FACTOR=1.0    # multiplier on live spread bps
BYBIT_TICKS=5              # iterations of the quoting loop
